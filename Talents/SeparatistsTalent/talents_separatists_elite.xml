<Talents>
  <Talent identifier="deep_elite_talent_separatists_soidier">
    <Icon texture="%ModDir%/Talents/shotgun.png" sheetindex="0,3" sheetelementsize="128,128" />
    <Description tag="talentdescription.deep_elite_talent_separatists_soidier"></Description>
    <!-- 精英士兵天赋 -->
    <!-- 多人游戏数值调整 -->
    <AbilityGroupEffect abilityeffecttype="None">
      <Abilities>
        <CharacterAbilityGiveStat stattype="MaximumHealthMultiplier" value="1.5" />
        <CharacterAbilityGiveStat stattype="MeleeAttackMultiplier" value="10" />
        <CharacterAbilityGiveStat stattype="MeleeAttackSpeed" value="1" />
        <CharacterAbilityGiveStat stattype="RangedAttackMultiplier" value="0.3" />
        <CharacterAbilityGiveStat stattype="MovementSpeed" value="0.4" />
        <CharacterAbilityModifyStat stattype="TeamAttackMultiplier" value="-1" />
      </Abilities>
    </AbilityGroupEffect>
    <!-- 抗性 -->
    <AbilityGroupInterval interval="5">
      <Abilities>
        <CharacterAbilityModifyResistance resistanceid="stun" multiplier="0.2" />
      </Abilities>
    </AbilityGroupInterval>
    <!-- 攻击时降低玩家40%伤害抗性 -->
    <AbilityGroupEffect abilityeffecttype="OnAttack">
      <Abilities>
        <CharacterAbilityApplyStatusEffects>
          <StatusEffects>
            <StatusEffect type="OnAbility" target="Character" setvalue="true" disabledeltatime="true" multiplyafflictionsbymaxvitality="true">
              <Affliction identifier="deep_gunwound_reduce_40" strength="1.0" />
            </StatusEffect>
          </StatusEffects>
        </CharacterAbilityApplyStatusEffects>
      </Abilities>
    </AbilityGroupEffect>
    <!-- 突击手 -->
    <AbilityGroupInterval interval="0.9">
      <Conditions>
        <AbilityConditionNearbyCharacterCount distance="800" targettypes="Enemy" count="1" />
      </Conditions>
      <Abilities>
        <CharacterAbilityModifyStat stattype="RangedAttackSpeed" value="0.2" />
        <CharacterAbilityModifyStat stattype="RangedAttackMultiplier" value="0.2" />
      </Abilities>
    </AbilityGroupInterval>
  </Talent>
