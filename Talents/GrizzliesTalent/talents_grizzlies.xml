<Talents>
  <Talent identifier="deep_talent_grizzlies_team_juggernaut">
    <Icon texture="%ModDir%/Talents/shotgun.png" sheetindex="0,3" sheetelementsize="128,128" />
    <Description tag="talentdescription.deep_talent_grizzlies_team_juggernaut"></Description>
    <!-- 士兵天赋 -->
    <!-- 数值调整 -->
    <AbilityGroupEffect abilityeffecttype="None">
      <Abilities>
        <CharacterAbilityGiveStat stattype="MaximumHealthMultiplier" value="1.7" />
        <CharacterAbilityGiveStat stattype="MeleeAttackMultiplier" value="10" />
        <CharacterAbilityGiveStat stattype="MeleeAttackSpeed" value="1" />
        <CharacterAbilityGiveStat stattype="MovementSpeed" value="0.3" />
        <CharacterAbilityModifyStat stattype="TeamAttackMultiplier" value="-1" />
        <CharacterAbilityModifyStat stattype="WeaponsSkillBonus" value="200" />
        <CharacterAbilityModifyStat stattype="RangedAttackMultiplier" value="0.3" />
      </Abilities>
    </AbilityGroupEffect>
    <!-- 攻击时降低玩家45%伤害抗性 -->
    <AbilityGroupEffect abilityeffecttype="OnAttack">
      <Abilities>
        <CharacterAbilityApplyStatusEffects>
          <StatusEffects>
            <StatusEffect type="OnAbility" target="Character" setvalue="true" disabledeltatime="true" multiplyafflictionsbymaxvitality="true">
              <Affliction identifier="deep_gunwound_reduce_45" strength="1.0" />
            </StatusEffect>
          </StatusEffects>
        </CharacterAbilityApplyStatusEffects>
      </Abilities>
    </AbilityGroupEffect>
    <!-- 抗性 -->
    <AbilityGroupInterval interval="5">
      <Abilities>
        <CharacterAbilityModifyResistance resistanceid="stun" multiplier="0.1" />
      </Abilities>
    </AbilityGroupInterval>
    <!-- 突击手 -->
    <AbilityGroupInterval interval="0.9">
      <Conditions>
        <AbilityConditionNearbyCharacterCount distance="800" targettypes="Enemy" count="1" />
      </Conditions>
      <Abilities>
        <CharacterAbilityModifyStat stattype="RangedAttackMultiplier" value="0.3" />
        <CharacterAbilityModifyStat stattype="RangedAttackSpeed" value="0.3" />
      </Abilities>
    </AbilityGroupInterval>
  </Talent>
