      <SubContainer capacity="1" maxstacksize="1">
        <Containable tags="deep_chip" hide="true" itempos="26,-1.5" setactive="true" containedspritedepth="0.45" depth="0.55" />
      </SubContainer>
      <SubContainer capacity="1" maxstacksize="1">
        <Containable tags="deep_chip" hide="true" itempos="26,-1.5" setactive="true" containedspritedepth="0.45" depth="0.55" />
      </SubContainer>
    </ItemContainer>
