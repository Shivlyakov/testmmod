    <Fabricate suitablefabricators="fabricator" requiredtime="10">
      <RequiredSkill identifier="weapons" level="80" />
      <RequiredItem identifier="physicorium_barrel" amount="4" />
      <RequiredItem identifier="physicorium_part" amount="4" />
      <RequiredItem identifier="meteorite_design" />
      <RequiredItem identifier="engineeringplastic" amount="2" />
      <RequiredItem identifier="glassfibre" />
    </Fabricate>
    <Deconstruct time="10">
      <Item identifier="physicorium" amount="6" />
    </Deconstruct>
    <InventoryIcon texture="%ModDir%/weapon/grenadelauncher/grenadelauncher45.png" sourcerect="1394,0,190,190" origin="0.5,0.5" />
    <Sprite texture="%ModDir%/weapon/grenadelauncher/grenadelauncher.png" sourcerect="6,166,265,83" depth="0.55" origin="0.5,0.5" />
    <Body width="260" height="80" density="10" />
    <!--双手拿-->
    <Holdable slots="RightHand+LeftHand" controlpose="true" holdpos="50,-10" aimpos="60,0" handle1="-55,-12" handle2="80,20" holdangle="-35" pickkey="Select" msg="ItemMsgEquipSelect">
      <!-- 禁用榴弹合约-->
      <StatusEffect type="OnActive" targettype="This,Character" setvalue="true" comparison="and">
        <Conditional deep_contract_no_gl_detect="gt 0.0" />
        <dropitem />
      </StatusEffect>
      <!-- 二战老兵合约-->
      <StatusEffect type="OnActive" targettype="This,Character" setvalue="true" comparison="and">
        <Conditional deep_contract_wwt_detect="gt 0.0" />
        <dropitem />
      </StatusEffect>
      <StatusEffect type="OnSecondaryUse" target="Character" SpeedMultiplier="0.6" setvalue="true" />
      <StatusEffect type="OnActive" target="Character" SpeedMultiplier="0.7" setvalue="true" />
      <StatusEffect type="Onactive" target="Character" setvalue="true">
        <GiveTalentInfo giverandom="false" talentidentifiers="deep_talent_all" />
      </StatusEffect>
      <!-- 有碎甲芯片时，增加1级穿甲-->
      <StatusEffect type="OnActive" interval="0.5" targettype="This,Character" targetitemcomponent="RangedWeapon" comparison="and" setvalue="true" penetration="0.1">
        <Conditional chip_bodyshot_detect="gt 10.0" />
      </StatusEffect>
      <!-- 有碎甲芯片时，增加1级穿甲-->
      <StatusEffect type="OnActive" interval="0.5" targettype="This,Character" targetitemcomponent="RangedWeapon" comparison="and" setvalue="true" penetration="0">
        <Conditional chip_bodyshot_detect="lt 10.0" />
      </StatusEffect>
    </Holdable>
    <!--可以背背上-->
    <Wearable slots="Bag" msg="ItemMsgPickUpUse" canbeselected="false" canbepicked="true" pickkey="Use">
      <sprite texture="%ModDir%/weapon/grenadelauncher/grenadelauncher.png" canbehiddenbyotherwearables="false" sourcerect="6,166,265,83" rotation="90" inheritlimbdepth="false" depth="0.2" limb="Torso" scale="0.5" origin="0.4,0.7" />
      <StatusEffect type="OnWearing" target="Character" SpeedMultiplier="0.8" setvalue="true" />
    </Wearable>
    <RangedWeapon reload="2.8" weapondamagemodifier="1" holdtrigger="true" barrelpos="110,3" spread="3" unskilledspread="6" combatPriority="80" drawhudwhenequipped="true" crosshairscale="0.5">
      <Crosshair texture="%ModDir%/weapon/sight/frontsight.png" sourcerect="8,0,253,255" />
      <CrosshairPointer texture="%ModDir%/weapon/sight/frontsight.png" sourcerect="273,0,237,249" />
      <ParticleEmitter particle="deep_muzzleflash" particleamount="1" velocitymin="0" velocitymax="0" />
      <ParticleEmitter particle="deep_plasmaspark" particlespersecond="10" anglemin="-20" anglemax="20" velocitymin="100" velocitymax="800" />
      <Sound file="%ModDir%/weapon/ammo/meteorite.ogg" type="OnUse" range="3000" selectionmode="Random" />
      <Sound file="%ModDir%/weapon/ammo/meteorite.ogg" type="OnUse" range="3000" frequencymultiplier="0.95" />
      <Sound file="%ModDir%/weapon/ammo/meteorite.ogg" type="OnUse" range="3000" frequencymultiplier="1.05" />
      <StatusEffect type="OnUse" target="This">
        <ParticleEmitter particle="deep_casingfirearm_127_99" particleamount="1" anglemin="90" anglemax="150" velocitymin="50" velocitymax="250" CopyEntityAngle="true" />
        <Explosion range="150.0" force="2" shockwave="true" smoke="false" flames="false" sparks="false" underwaterbubble="false" camerashake="100" />
      </StatusEffect>
      <StatusEffect type="OnUse" target="This" Condition="-200" />
      <StatusEffect type="NotInWater" target="This" Condition="5" />
      <StatusEffect type="InWater" target="This" Condition="130" />
      <!--"Use" contained magazine to spawn projectiles.-->
      <StatusEffect type="OnUse" target="Contained">
        <Use />
      </StatusEffect>
      <StatusEffect type="OnUse" Condition="-80" target="This">
        <RequiredItems items="chip_unstable_gunpowder" type="Contained" />
      </StatusEffect>
      <StatusEffect type="OnUse" Condition="-120" target="This">
        <RequiredItems items="chip_oc_gun" type="Contained" />
      </StatusEffect>
      <RequiredItems items="deep_meteorite_mag,tsm_grenade_mag" type="Contained" msg="ItemMsgAmmoRequired" />
      <RequiredSkill identifier="weapons" level="80" />
      <StatusEffect type="OnBroken" target="This" Condition="3000" disabledeltatime="true" delay="2" stackable="false" />
      <StatusHUD drawhudwhenequipped="false" overlaycolor="0,0,0,0" range="3000" thermalgoggles="false" showdeadcharacters="false" showtexts="false" />
    </RangedWeapon>
    <ItemContainer capacity="1" maxstacksize="1" hideitems="false" containedstateindicatorslot="0" containedstateindicatorstyle="bullet">
      <Containable items="deep_meteorite_mag,tsm_grenade_mag" hide="false" itempos="-11,-10" rotation="0">
        <!-- Locks the ammunition, preventing the person from adding more or removing. -->
        <StatusEffect type="OnInserted" target="Contained" NonInteractable="true" delay="0.01" setvalue="true" />
        <!-- Unlocks the ammunition, allowing the person to add more or remove. -->
        <StatusEffect type="OnInserted" target="Contained" NonInteractable="false" delay="2" setvalue="true" />
        <!-- 弹匣隐藏 -->
        <StatusEffect type="OnInserted" target="Contained" spritecolor="255,255,255,0" delay="0.01" setvalue="true" disabledeltatime="true" />
        <!-- 弹匣显示 -->
        <StatusEffect type="OnInserted" target="Contained" spritecolor="255,255,255,255" delay="0.5" setvalue="true" disabledeltatime="true" />
        <StatusEffect type="OnInserted" target="This" holdpos="30,-20" holdangle="10" handle2="5,-30" delay="0.01" condition="0" setvalue="true" />
        <!-- Puts the hand back. -->
        <StatusEffect type="OnInserted" target="This" handle2="-20,-5" delay="0.3" setvalue="true" />
        <!-- Sets gun's and hand's position, makes a sound. -->
        <StatusEffect type="OnInserted" target="This" handle2="-40,-40" holdangle="40" holdpos="15,-15" aimangle="45" aimpos="25,0" delay="0.7" condition="0" setvalue="true" forceplaysounds="true">
          <!-- 声音文件 <Sound file="%ModDir%/MachinePistol/MachinePistolMagInsert.ogg" type="OnUse" range="1000" /> -->
        </StatusEffect>
        <!-- Puts the hand back. -->
        <StatusEffect type="OnInserted" target="This" handle2="18,-8" delay="1.3" setvalue="true" />
        <!-- Puts the hand on the eject port. -->
        <StatusEffect type="OnInserted" target="This" handle1="1,10" delay="1.1" setvalue="true" />
        <!-- Eject port work. -->
        <StatusEffect type="OnInserted" target="This" handle1="-30,10" delay="1.5" forceplaysounds="true">
          <!-- 声音文件 <Sound file="%ModDir%/DeadeyeCarbine/DeadeyeCarbineEjectPortClose.ogg" type="OnUse" range="1000" /> -->
        </StatusEffect>
        <!-- End. Sets everything to default values. -->
        <StatusEffect type="OnInserted" target="This" handle1="-55,-12" handle2="80,20" holdangle="-35" holdpos="50,-10" aimangle="0" aimpos="60,0" delay="2" setvalue="true" />
        <!-- If the magazine is removed/swapped, sets the condition to 1. -->
        <StatusEffect type="OnInserted" target="This" Condition="3000" disabledeltatime="true" delay="2" stackable="false" />
        <StatusEffect type="OnInserted" target="This" setValue="true" disabledeltatime="true" delay="0.01">
          <sound file="%ModDir%/weapon/ammo/akre.ogg" type="OnUse" range="500.0" loop="false" volume="1" />
          <ParticleEmitter particle="fleshsmoke" particlespersecond="2" scalemin="1" scalemax="2" />
        </StatusEffect>
      </Containable>
      <SlotIcon slotindex="0" texture="%ModDir%/misc/ui.png" sourcerect="10,8,64,57" origin="0.5,0.5" />
      <SlotIcon slotindex="1" texture="%ModDir%/misc/ui.png" sourcerect="10,70,85,40" origin="0.5,0.5" />
